<!doctype html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
	<meta name="format-detection" content="telephone=no">
	<title>veeva-slide</title>

	<!-- build:css ../shared/styles/main.css -->
	<!-- inject:css -->
	<!-- endinject -->
   <link rel="stylesheet" href="../shared/styles/main.css">
   <!-- endbuild -->
   <!-- build:css assets/styles/slide.css -->
   <link rel="stylesheet" href="assets/styles/slide.css" />
   <!-- endbuild -->
</head>

<body>
<main id="rheum_hcp_iva_U3_2" class="slide">
   {{> header }}
   <div class="content">
      <a class="Pathogenesis1 popup-open show-invisible" data-target="Pathogenesis1"></a>
      <a class="Pathogenesis2 popup-open show-invisible" data-target="Pathogenesis2"></a>
      <a class="Pathogenesis3 popup-open show-invisible" data-target="Pathogenesis3"></a>
      <a class="Pathogenesis4 popup-open show-invisible" data-target="Pathogenesis4"></a>
      <a class="next-button orange show-invisible" data-target="3_rheum_hcp_iva_U3"></a>
   </div>
   {{> references }}
   {{> isi }}
      <div class="popups-container">
		<div class="popup-overlay"></div>
      {{>popup popupId="Pathogenesis1" imgUrl="assets/images/Pathogenesis1.png"}}
      {{>popup popupId="Pathogenesis2" imgUrl="assets/images/Pathogenesis2.png"}}
      {{>popup popupId="Pathogenesis3" imgUrl="assets/images/Pathogenesis3.png"}}
      {{>popup popupId="Pathogenesis4" imgUrl="assets/images/Pathogenesis4.png"}}
   </div>
</main>

<!-- build:js ../shared/scripts/main.js -->
<!-- inject:js -->
<!-- endinject -->
<script src="../shared/scripts/jquery.js"></script>
<script src="../shared/scripts/libs.js"></script>
<script src="../shared/scripts/libs/veeva-library.js"></script>
<script src="../shared/scripts/libs/veeva-utils.js"></script>
<script src="../shared/scripts/menuHandling.js"></script>
<script src="../shared/scripts/main.js"></script>
<!-- endbuild -->

<!-- build:js assets/scripts/slide.js -->
<script src="assets/scripts/slide.js"></script>
<!-- endbuild -->

</body>
</html>
