<!doctype html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
	<meta name="format-detection" content="telephone=no">
	<title>veeva-slide</title>

	<!-- build:css ../shared/styles/main.css -->
	<!-- inject:css -->
	<!-- endinject -->
   <link rel="stylesheet" href="../shared/styles/main.css">
   <!-- endbuild -->
   <!-- build:css assets/styles/slide.css -->
   <link rel="stylesheet" href="assets/styles/slide.css" />
   <!-- endbuild -->
</head>

<body>
<main id="rheum_hcp_iva_U3_45" class="slide">
   {{> header
      tab1="38_rheum_hcp_iva_U3"
      tab2="41_rheum_hcp_iva_U3"
      tab3="42_rheum_hcp_iva_U3"
   }}
   <div class="content">
      <a class="acthar-treatment popup-open show-invisible" data-target="acthar-treatment"></a>
      <a class="physician-measured popup-open show-invisible" data-target="physician-measured"></a>
      <a class="improvement popup-open show-invisible" data-target="improvement"></a>
      <a class="next-button orange show-invisible" data-target="46_rheum_hcp_iva_U3"></a>
      <div class="InactiveHeader_35"></div>
      <div class="menuDropdown_35">
         <div class="activeHeader_35"></div>
         <a class="clickArea1_35" data-target="38_rheum_hcp_iva_U3"></a>
         <a class="clickArea3_35" data-target="47_rheum_hcp_iva_U3"></a>
      </div>

   </div>
   {{> references }}
   {{> isi }}
   <div class="popups-container">
		<div class="popup-overlay"></div>
      {{>popup popupId="acthar-treatment" imgUrl="assets/images/45_Popup-window-1.png"}}
      {{>popup popupId="physician-measured" imgUrl="assets/images/45_Popup-window-2.png"}}
      {{>popup popupId="improvement" imgUrl="assets/images/45_Popup-window-3.png"}}
   </div>
</main>

<!-- build:js ../shared/scripts/main.js -->
<!-- inject:js -->
<!-- endinject -->
<script src="../shared/scripts/jquery.js"></script>
<script src="../shared/scripts/libs.js"></script>
<script src="../shared/scripts/libs/veeva-library.js"></script>
<script src="../shared/scripts/libs/veeva-utils.js"></script>
<script src="../shared/scripts/menuHandling.js"></script>
<script src="../shared/scripts/main.js"></script>
<!-- endbuild -->

<!-- build:js assets/scripts/slide.js -->
<script src="assets/scripts/slide.js"></script>
<!-- endbuild -->

</body>
</html>
